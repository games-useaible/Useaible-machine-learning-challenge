<div class="panel panel-primary  k-content">
    <div class="panel-heading"><span data-bind="text: TensorflowSelected()? 'Tensor Flow' : 'Encog'"></span></div>
    <div class="panel-body">
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#secondTab1" aria-controls="secondTab1" role="tab" data-toggle="tab">Settings</a></li>
            <li role="presentation"><a href="#secondTab2" aria-controls="secondTab2" role="tab" data-toggle="tab">Session Data</a></li>
        </ul>
        <div class="tab-content">
            <div data-bind="visible: HeadToHeadSelected" div role="tabpanel" class="tab-pane active" id="secondTab1">
                <div class="sidebarHeight row">
                    @*<button class="btn btn-info full-width margin-bottom" data-bind="click: TfPlayGame.bind($root, 'tensorFlow'), visible: ReplayButton, enable: true">Play Game</button>*@
                    <div id="player2-settings-container"  class="col-md-12">
                        @*<div class="row">
                            <div class="col-md-6">
                                <h4 class="input-label">Starting Altitude</h4>
                                <select class="new-panels margin-bottom" data-bind="enable: DonePlaying, options: AltitudeOptions, optionsText: 'Name', optionsValue: 'Value', value: SelectAltitude"></select>
                            </div>
                            <div class="col-md-6">
                                <h4 class="input-label">Starting Fuel</h4>
                                <select class="new-panels margin-bottom" data-bind="enable: DonePlaying, options: FuelOptions, value: SelectFuel"></select>
                            </div>
                        </div>
                        <div class="secondPanelPlaceholder">
                        </div>*@
                    </div>
                </div>
            </div>
            <div data-bind="visible: HeadToHeadSelected"  div role="tabpanel" class="tab-pane" id="secondTab2">

                <div class="sidebarHeight row">
                    <div class="col-md-12">
                        <div class="tensorflowTable">
                            <table id="headToHeadTFTbl" class="table table-hover" style="background: #ffffff">
                                <caption><span data-bind="text: TensorflowSelected()? 'Tensor Flow' : 'Encog'"></span></caption>
                                <thead>
                                    <tr>
                                        <th>Status</th>
                                        <th>Session</th>
                                        <th>Score</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach: $root.GameQueue">
                                    <tr>
                                        <!--ko if: Type() == 'tensorflow'-->
                                        <td data-bind="html: Icon"></td>
                                        <td data-bind="text: Session"></td>
                                        <td data-bind="text: Score"></td>
                                        <!--/ko-->
                                        <!--ko if: Type() == 'encog'-->
                                        <td data-bind="html: Icon"></td>
                                        <td data-bind="text: Session"></td>
                                        <td data-bind="text: Score"></td>
                                        <!--/ko-->
                                    </tr>
                                </tbody>
                            </table>
                            <div id="tensorFlowWaitingDiv" data-bind="visible: $root.TensorFlowDonePlaying() == false">
                                <label>Waiting Results</label><br />
                                <label>Session:</label> <span data-bind="text: $root.SessionWaitTextTF"></span><br />
                                <label>Time Elapse:</label> <span data-bind="text: $root.WaitTimeElapseTF"></span>
                                <div class="progress progress-striped active">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                                    </div>
                                </div>
                            </div>
                            <div id="encogWaitingDiv" data-bind="visible: $root.EncogDonePlaying() == false">
                                <label>Waiting Results</label><br />
                                <label>Session:</label> <span data-bind="text: $root.SessionWaitTextEncog"></span><br />
                                <label>Time Elapse:</label> <span data-bind="text: $root.WaitTimeElapseEncog"></span>
                                <div class="progress progress-striped active">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script></script>
