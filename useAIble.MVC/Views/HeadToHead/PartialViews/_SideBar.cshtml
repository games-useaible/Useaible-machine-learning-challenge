<div class="panel-heading" id="firstSideBar"><span><i class="fa fa-cogs" aria-hidden="true"></i></span> Settings </div>
<div class="panel-body">
    <ul class="nav nav-tabs" role="tablist">
        <li role="presentation" class="active"><a href="#tab1" aria-controls="htab1" role="tab" data-toggle="tab">Settings</a></li>
        <li role="presentation"><a href="#tab2" aria-controls="tab2" role="tab" data-toggle="tab">Session Data</a></li>
    </ul>
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="tab1">
            <div class="sidebarHeight row">
                <div class="col-md-12">
                    @if (ViewBag.Game == "LunarLander")
                    {
                        @Html.Partial("PartialViews/LunarLander/_SettingsTab")
                    }
                    else if (ViewBag.Game == "Maze")
                    {
                        @Html.Partial("PartialViews/Maze/_SettingsTab")
                    }
                    else if (ViewBag.Game == "LogisticsSimulation")
                    {
                        @Html.Partial("PartialViews/LogisticsSimulation/_SettingsTab")
                    }
                </div>
            </div>
        </div>
        <div role="tabpanel" class="tab-pane" id="tab2">
            <div class="sidebarHeight row">
                <div class="col-md-12">
                    @if (ViewBag.Game == "LunarLander")
                    {
                        @Html.Partial("PartialViews/LunarLander/_SessionsTab")
                    }
                    else
                    {
                        @Html.Partial("PartialViews/Maze/_SessionsTab")
                    }
                </div>
            </div>
        </div>
    </div>
</div>